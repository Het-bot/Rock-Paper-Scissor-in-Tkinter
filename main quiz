# A code for Quiz which includes 10 questions out of which 5 are single correct and 5 are multiple correct
# The quiz calculates the score and gives a detailed performance report 
import tkinter as tk
from tkinter.ttk import *
from tkinter import *
import time

c_s=0          #no. of correct answer of single correct questions
c_m=0         #no. of correct answer of multiple correct questions      
i_s=0           #no. of incorrect answer of single correct questions
i_m=0          #no. of correct answer of multiple correct questions
n_s=0          #answer not attempted for single correct questions
n_m=0        #answer not attempted for multiple correct questions
score_s=0
score_m=0

m=tk.Tk()

l1=Label(m,text="Quiz ",font=('Lucida Handwriting',100))
l1.pack()  

def credit():
    global l_cr
    l_cr=Label(m,text="This project is created by Het N. Patel, Sneh M. Patel and Vishrut H.Patel",font=('Times New Roman',20))
    l_cr.pack()
b=Button(m,text="About Us",command=credit, font=('Times New Roman',20))
b.pack()

def next():
    l1.destroy()
    b1.destroy()
