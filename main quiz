                                                                subplot6 = fig6.add_subplot(111) 
                                                                labels6 = 'Correct','Incorrect','Not Attempted'
                                                                pieSizes6 = [c_m,i_m,n_m]
                                                                explode6 = (0.1, 0.1,0.1)
                                                                subplot6.pie(pieSizes6, explode=explode6, labels=labels6, autopct='%1.1f%%', shadow=True, startangle=90) 
