                                                            def pic(c_s,c_m,i_s,i_m,n_s,n_m,score_s,score_m,c,i,n,a,score,tot,per,acc,per_s,per_m,acc_s,acc_m):
                                                                canvas1 = tk.Canvas(m, width = 20000, height = 20000,bg="deep sky blue")
                                                                canvas1.pack()  
      
                                                      
                                                                Label(m,text="★ Total Questions").place(x=100,y=0)
                                                                Label(m,text="★ Score").place(x=700,y=0)
                                                                Label(m,text="★ Percentage").place(x=100,y=600)
                                                                Label(m,text="★ Accuracy").place(x=700,y=600)
                                                                Label(m,text="★ Single correct").place(x=100,y=1200)
                                                                Label(m,text="★ Multiple Correct").place(x=700,y=1200)
                                                                fig1 = Figure(figsize=(5,4), dpi=100) 
                                                                subplot1 = fig1.add_subplot(111) 
                                                                labels1 = 'Correct', 'Incorrect', 'Not attempted' 
                                                                pieSizes1 = [c,i,n]
                                                                explode1 = (0.1, 0.1, 0.1)  
                                                                subplot1.pie(pieSizes1, explode=explode1, labels=labels1, autopct='%1.1f%%', shadow=True, startangle=90) 
                                                                subplot1.axis('equal')  
                                                                pie1 = FigureCanvasTkAgg(fig1, m) 
                                                                pie1.get_tk_widget().place(x=0,y=60)
                                                                   
                                                                fig2 = Figure(figsize=(5,4), dpi=100) 
                                                                subplot2= fig2.add_subplot(111) 
                                                                labels2 = 'Single', 'Multiple' 
                                                                pieSizes2 = [score_s,score_m]
                                                                explode2 = (0.1, 0.1)
                                                                subplot2.pie(pieSizes2, explode=explode2, labels=labels2, autopct='%1.1f%%', shadow=True, startangle=90) 
                                                                subplot1.axis('equal') 
                                                                pie2 = FigureCanvasTkAgg(fig2, m) 
                                                                pie2.get_tk_widget().place(x=600,y=60)
                                                                fig3= Figure(figsize=(5,4), dpi=100) 
                                                                subplot3 = fig3.add_subplot(111) 
                                                                labels3 = 'Single', 'Multiple', 
                                                                pieSizes3 = [per_s,per_m]
                                                                explode3 = (0.1, 0.1)  
                                                                subplot3.pie(pieSizes3, explode=explode3, labels=labels3, autopct='%1.1f%%', shadow=True, startangle=90) 
                                                                subplot3.axis('equal')  
                                                                pie3 = FigureCanvasTkAgg(fig3, m) 
