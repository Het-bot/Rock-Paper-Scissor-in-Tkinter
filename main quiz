                                def submit6(c_s,c_m,i_s,i_m,n_s,n_m,score_s,score_m,clr_fg_1,clr_fg_2,clr_fg_3,clr_fg_4,clr_fg_5):
                                    A6= str(Checkvar1.get())
                                    B6= str(Checkvar2.get())
                                    C6= str(Checkvar3.get())
                                    D6= str(Checkvar4.get())

                                    if (A6=="0" and B6=="1" and C6=="0" and D6=="1"):
                                        score_m+=4
                                        c_m+=1
                                        clr_fg_6 ="green2"        
                                    elif (A6=="0" and B6=="0" and C6=="0" and D6=="0") :
                                        score_m+=0
                                        n_m+=1
                                        clr_fg_6 ="gray73"
                                        A6="N.A."
        
                                    else:
                                        score_m-=1
                                        i_m+=1
                                        clr_fg_6 ="orangered"
                                    if A6=="1" and B6=="0" and C6=="0" and D6=="0":
                                        A6="A"
                                    elif A6=="0" and B6=="1" and C6=="0" and D6=="0":
                                        A6="B"
                                    elif A6=="0" and B6=="0" and C6=="1" and D6=="0":
                                        A6="C"
                                    elif A6=="0" and B6=="0" and C6=="0" and D6=="1":
                                        A6="D"
                                    elif A6=="1" and B6=="1" and C6=="0" and D6=="0":
                                        A6="A,B"
                                    elif A6=="1" and B6=="0" and C6=="1" and D6=="0":
                                        A6="A,C"
                                    elif A6=="1" and B6=="0" and C6=="0" and D6=="1":
                                        A6="A,D"
                                    elif A6=="0" and B6=="1" and C6=="1" and D6=="0":
                                        A6="B,C"    
                                    elif A6=="0" and B6=="1" and C6=="0" and D6=="1":
                                        A6="B,D"
                                    elif A6=="0" and B6=="0" and C6=="1" and D6=="1":
                                        A6="C,D"
                                    elif A6=="1" and B6=="1" and C6=="1" and D6=="0":
                                        A6="A,B,C"
                                    elif A6=="1" and B6=="1" and C6=="0" and D6=="1":
                                        A6="A,B,D"
                                    elif A6=="1" and B6=="0" and C6=="1" and D6=="1":
                                        A6="A,C,D"
                                    elif A6=="0" and B6=="1" and C6=="1" and D6=="1":
                                        A6="B,C,D"
                                    elif A6=="1" and B6=="1" and C6=="1" and D6=="1":
                                        A6="A,B,C,D"
                                    else:
                                        A6="N.A."
                                
      
