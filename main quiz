                global n_s
                global score_s
                global clr_fg_1

                if a1=="2":
                    score_s+=4
                    clr_fg_1="green2"
                    c_s+=1
                elif a1=="" or a1=="5":
                    score_s+=0
                    n_s+=1
                    clr_fg_1="gray73"
                    a1="N.A."
                else:
                    score_s-=1
                    i_s+=1
                    clr_fg_1="orangered"
                if a1=="1":
                    a1="A"
