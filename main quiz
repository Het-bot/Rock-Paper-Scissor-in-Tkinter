                                                    def res(c_s,c_m,i_s,i_m,n_s,n_m,score_s,score_m,clr_fg_1,clr_fg_2,clr_fg_3,clr_fg_4,clr_fg_5,clr_fg_6,clr_fg_7,clr_fg_8,clr_fg_9,clr_fg_10):
                                                        l31=Label(m,text="Calculating scores...",font=('Times New Roman',10),bg="deep sky blue")
                                                        l31.pack()
                                                        progress['value'] = 20
                                                        m.update_idletasks()
                                                        time.sleep(0.1)
                                                        progress['value'] = 40
                                                        m.update_idletasks() 
                                                        time.sleep(0.1) 
                                                        progress['value'] = 60
                                                        m.update_idletasks() 
                                                        time.sleep(0.1) 
                                                        progress['value'] = 80
                                                        m.update_idletasks() 
                                                        time.sleep(0.1) 
