                                                        i = i_s + i_m
                                                        n = n_s + n_m
                                                        a = i + c
                                                        tot = c + n + i
                                                        score = score_s +score_m
                                                        per = (score/40)*100
                                                        try:
                                                            acc = 100-((i/c)*100)
                                                        except :
                                                            acc=0
                                                        per_s = (score_s/20)*100
                                                        try :
                                                            acc_s = 100-((i_s/c_s)*100)
                                                        except :
                                                            acc_s=0
                                                        per_m = (score_m/20)*100
